# K6 可视化测试平台环境变量配置

# 后端服务端口
VIZ_PORT=8080

# 环境配置
NODE_ENV=development

# Grafana 配置（可选）
GRAFANA_URL=http://localhost:3000
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# InfluxDB 配置（可选）
INFLUXDB_URL=http://localhost:8086
INFLUXDB_DB=k6
INFLUXDB_USER=
INFLUXDB_PASSWORD=

# 报告配置
REPORTS_RETENTION_DAYS=30
